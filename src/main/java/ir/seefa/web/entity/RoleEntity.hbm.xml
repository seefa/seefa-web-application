<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ir.seefa.web.entity.RoleEntity" table="Role" schema="seefasitev1">
        <id name="roleId">
            <column name="roleId" sql-type="int(11)"/>
        </id>
        <property name="roleName">
            <column name="roleName" sql-type="varchar(75)" length="75"/>
        </property>
        <property name="roleDisabled">
            <column name="roleDisabled" sql-type="tinyint(1)"/>
        </property>
        <property name="roleType">
            <column name="roleType" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>